{
	"appId": "io.konveier.timo",
	"appName": "Brite",
	"webDir": "www",
	"bundledWebRuntime": true,
	"ios": {
		"path": "ios/App/Brite.xcworkspace",
		"includePlugins": [
			"cordova-annotated-plugin-android",
			"cordova-plugin-background-mode",
			"cordova-ios-plugin-userdefaults",
			"cordova-launch-review",
			"cordova-plugin-3dtouch",
			"cordova-plugin-browser",
			"cordova-plugin-customurlscheme",
			"cordova-plugin-device",
			"cordova-plugin-file",
			"cordova-plugin-file-transfer",
			"cordova-plugin-geolocation",
			"cordova-plugin-globalization",
			"cordova-plugin-idfa",
			"cordova-plugin-inappbrowser",
			"cordova-plugin-insomnia",
			"cordova-plugin-ionic-keyboard",
			"cordova-plugin-keychain-storage",
			"cordova-plugin-localization-strings",
			"cordova-plugin-media",
			"cordova-plugin-minify",
			"cordova-plugin-network-information",
			"cordova-plugin-persist-localstorage",
			"cordova-plugin-purchase",
			"cordova-plugin-screen-orientation",
			"cordova-plugin-sign-in-with-apple",
			"cordova-plugin-smartlook",
			"cordova-plugin-splashscreen",
			"cordova-plugin-statusbar",
			"cordova-plugin-taptic-engine",
			"cordova-plugin-theme-detection",
			"cordova-plugin-webviewcolor",
			"cordova-plugin-whitelist",
			"cordova-plugin-x-socialsharing",
			"cordova.plugins.diagnostic",
			"zalarudo-widget-refresh"
		]
	},
	"plugins": {
		"Keyboard": {
			"resize": "body",
			"style": "DARK"
		},
		"SplashScreen": {
			"launchShowDuration": 3000,
			"launchAutoHide": true,
			"launchFadeOutDuration": 3000,
			"backgroundColor": "#ffffffff",
			"androidSplashResourceName": "splash",
			"androidScaleType": "CENTER_CROP",
			"showSpinner": true,
			"androidSpinnerStyle": "large",
			"iosSpinnerStyle": "small",
			"spinnerColor": "#999999",
			"splashFullScreen": true,
			"splashImmersive": true,
			"layoutName": "launch_screen",
			"useDialog": true
		}
	},
	"cordova": {
		"preferences": {
			"DisallowOverscroll": "true",
			"UIWebViewBounce": "true",
			"SplashScreenDelay": "1000",
			"IOS_UNIFORM_TYPE_IDENTIFIER": "public.image",
			"AutoHideSplashScreen": "true",
			"ShowSplashScreenSpinner": "false",
			"FadeSplashScreen": "true",
			"FadeSplashScreenDuration": "500",
			"SuppressesLongPressGesture": "true",
			"ServiceWorker": "sw.js",
			"BackgroundColor": "0x000000ff",
			"KeyboardStyle": "dark",
			"KeyboardResizeMode": "body",
			"KeyboardResize": "false",
			"StatusBarStyle": "blackopaque",
			"iosPersistentFileLocation": "Library",
			"InAppBrowserOverrideUserAgent": "Mozilla/5.0 Google",
			"UseSwiftLanguageVersion": "5",
			"iosExtraFilesystemsвы": "library,library-nosync,documents,documents-nosync,cache,bundle,root",
			"AllowInlineMediaPlayback": "true",
			"EnableWebGL": "false",
			"WindowSize": "1200x1000",
			"KeyboardDisplayRequiresUserAction": "false",
			"HideKeyboardFormAccessoryBar": "true",
			"PLAY_SERVICES_VERSION": "17.0.+"
		}
	}
}
